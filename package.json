{
  "name": "fastify-typescript-api",
  "version": "1.0.0",
  "description": "A Node.js TypeScript API application using Fastify and Jest",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js,json}\""
  },
  "keywords": [
    "fastify",
    "typescript",
    "api",
    "jest"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "jose": "^6.1.3",
    "mssql": "^12.2.0",
    "redis": "^5.10.0",
    "redlock-universal": "^0.8.1",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/mssql": "^9.1.8",
    "@types/node": "^24.10.1",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
